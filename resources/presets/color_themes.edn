{:color-themes
 {;; best 主题：商业美化配色提取
  :best {:pool-lane-fill-colors {:preset :best-aesthetic}
         :font-color :black
         :pool-title-font-color :white
         :lane-title-font-color :black
         :background-fill :white
         :task-fill [:colors :color-pool :best-blue]
         :task-stroke :gray600
         :lane-body-font-color :black
         :lane-body-fill-color [:colors :color-pool :best-gray-light]
         :connector-stroke :gray400
         :connector-width 1
         :connector-opacity 0.25}

  ;; default 主题：作为"最全"兜底主题，使用现代科技风配色
  :default
  {:desc "Modern professional default theme with clean slate/white palette"
   :pool-lane-fill-colors {:preset :tech-modern}
   ;; 基础与背景
   :background-fill :white
   ;; 文本颜色（按组件语义区分）
   :font-color "#1E293B" ; Slate 800
   :pool-title-font-color :white
   :lane-title-font-color "#334155" ; Slate 700
   :lane-body-font-color "#334155"

   ;; 结构颜色
   :pool-fill-color "#334155" ; Slate 700
   :lane-fill-color "#F1F5F9" ; Slate 100
   :lane-body-fill-color :white

   ;; 组件填充/描边
   :task-fill :white ; Clean look, no more blue blobs
   :task-stroke "#3B82F6" ; Blue 500 stroke for definition
   :element-outline-width 2 ; Slightly thicker stroke for modern feel

   ;; 连接线样式
   :connector-stroke "#64748B" ; Slate 500
   :connector-width 1
   :connector-opacity 1.0}

